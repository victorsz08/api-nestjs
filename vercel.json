{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node",
            "config": {
                "nodeVersion": "20.x"
            }
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/main.ts"
        }
    ]
}