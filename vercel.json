{
  "version": 2,
  "builds": [
    {
      "src": "src/main.js",
      "use": "@vercel/node",
      "config": {
        "deployCommand": "npm install && npm run build && npm run start:prod",
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ]
}